#define TST 0

/ {
  keymap {
    compatible = "zmk,keymap";

    test {
      display-name = "test";
      bindings = <
        &kp ESC        &kp N1          &kp N2       &kp N3         &kp N4           &kp N5               &kp N6          &kp N7
         &kp N8        &kp N9       &kp N0          &kp BSPC
        &kp TAB        &kp Q           &kp W        &kp E          &kp R            &kp T                &kp Y           &kp U
         &kp I         &kp O        &kp P           &kp RSHFT
        &kp LALT       &kp A           &kp S        &kp D          &kp F            &kp G                &kp H           &kp J
         &kp K         &kp L        &kp RET         &kp RALT
        &kp LCTRL      &kp Z           &kp X        &kp C          &kp V            &kp B                &kp N           &kp M
         &kp COMMA     &kp DOT      &kp APOS        &kp RCTRL
                                                    &kp ESC        &kp TAB          &kp SPACE            &kp RET         &kp BSPC     &kp DEL
      >;
    };
  };
